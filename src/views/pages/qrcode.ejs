<%- include('./style.ejs') %>

<div class="container">
	<div class="post">
		<div class="header_post">
			<img
				style="background-size: cover"
				src="data:image/jpeg;base64,"
				alt=""
			/>
		</div>

		<div class="body_post">
			<div class="post_content">
				<h1><%= ordType %></h1>

				<% observations.forEach(function (observation) { %>

				<p>- <%= observation %></p>

				<% }); %>

				<div class="container_infos">
					<div class="postedBy">
						<span>Nombre/Placa</span>
						<%= name %>
					</div>

					<div class="container_tags">
						<span>Estado</span>
						<div class="tags">
							<ul>
								<%- status %>
							</ul>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
